<?xml version="1.0" encoding="utf-8" ?>
<demo desc="测试接口">
    <hello-world title="基础使用">
        select 'hello world' key
    </hello-world>
    <cache cache="5000" title="使用缓存(cache单位毫秒)" body="{a:1,b:2}">
        select 'cache' cache,now() now;
    </cache>
    <param title="参数获取与处理">
        select :param param
    </param>
    <condition title="条件处理" desc="传递param参数判断与10的关系">
        select
        <if test="param&gt;10">
            'gt 10' param_status
        </if>
        <if test="param&lt;10">
            'lt 10' param_status
        </if>
        <if test="param==10">
            'eq 10' param_status
        </if>
        <if>
            ,:param param
        </if>
    </condition>
    <insert title="插入数据" type="modify">
        insert into demo(content) values (:content)
    </insert>
</demo>